\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{mathsymbolzhcn}[2018/11/11 v1.00 well-designed (TeX)]

\RequirePackage{fontspec}
\newfontfamily\unicodefont{mathsymbol-zh-cn.otf}

\newcommand{\zhparaleq}{\text{\unicodefont\symbol{"E000}}}  % 平行且等于
\newcommand{\zhparalogram}{\text{\unicodefont\symbol{"E001}}} % 平行四边形
\newcommand{\zhtriangle}{\text{\unicodefont\symbol{"E002}}} % 三角形
\newcommand{\zhsimilar}{\text{\unicodefont\symbol{"E003}}} % 相似于
\newcommand{\zhcongruent}{\text{\unicodefont\symbol{"E004}}}  % 全等于
\newcommand{\zhparallel}{\text{\unicodefont\symbol{"E005}}}  % 平行
\newcommand{\zhsubsetneqq}{\text{\unicodefont\symbol{"E006}}}  %中文真子集
\newcommand{\zhsubset}{\text{\unicodefont\symbol{"E007}}} %中文子集
\newcommand{\zhsupsetneqq}{\text{\unicodefont\symbol{"E008}}} %中文反向真子集
\newcommand{\zhsupset}{\text{\unicodefont\symbol{"E009}}}  %中文反向子集


